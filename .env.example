# Shared Environment Variables for StudyDash
# Copy this file to .env and fill in your actual values

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=studydash
DB_USER=postgres
DB_PASSWORD=

# Backend API Configuration
PORT=3001
NODE_ENV=development
JWT_SECRET=
API_BASE_URL=http://localhost:3001

# Frontend Configuration (Vite variables must start with VITE_)
VITE_API_BASE_URL=http://localhost:3001
VITE_APP_NAME=StudyDash
VITE_APP_VERSION=1.0.0

# Development
DEBUG=true
VITE_DEBUG=true